SOURCES_SVC.h = 
SOURCES.x = wolf_rpc.x

TARGETS_SVC.c = wolf_rpc_svc.c wolf_rpc_server.c wolf_rpc_xdr.c wolf_pmap_clnt.c
TARGETS_CLNT.c = wolf_rpc_clnt.c wolf_rpc_client.c wolf_rpc_xdr.c wolf_pmap_clnt.c
TARGETS = wolf_rpc.h wolf_rpc_xdr.c wolf_rpc_clnt.c wolf_rpc_svc.c wolf_rpc_client.c wolf_rpc_server.c

OBJECTS_CLNT = $(SOURCES_CLNT.c:%.c=%.o) $(TARGETS_CLNT.c:%.c=%.o)
OBJECTS_SVC = $(SOURCES_SVC.c:%.c=%.o) $(TARGETS_SVC.c:%.c=%.o)
# Compiler flags 

CFLAGS += -g 
LDLIBS += -lnsl
RPCGENFLAGS = 

# Targets 

all : $(CLIENT) $(SERVER)

$(TARGETS) : $(SOURCES.x) 
	rpcgen $(RPCGENFLAGS) $(SOURCES.x)

$(OBJECTS_CLNT) : $(SOURCES_CLNT.c) $(SOURCES_CLNT.h) $(TARGETS_CLNT.c) 

$(OBJECTS_SVC) : $(SOURCES_SVC.c) $(SOURCES_SVC.h) $(TARGETS_SVC.c) 

$(CLIENT) : $(OBJECTS_CLNT) 
	$(LINK.c) -o $(CLIENT) $(OBJECTS_CLNT) $(LDLIBS) 

$(SERVER) : $(OBJECTS_SVC) 
	$(LINK.c) -o $(SERVER) $(OBJECTS_SVC) $(LDLIBS)

 clean:
	 $(RM) -rf *.o $(CLIENT) $(SERVER)

